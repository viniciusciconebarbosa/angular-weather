{
  "version": 2,
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/demo",
  "routes": [
    {
      "src": "/(assets/.+)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.(?:js|css|ico|json|txt|html|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)$)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}